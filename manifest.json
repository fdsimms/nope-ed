{
  "manifest_version": 2,

  "name": "Nope-Ed",
  "description": "",
  "version": "0.0.0.1",

  "content_scripts": [
      {
        "matches": ["https://*.nytimes.com/"],
        "js": ["main.js"],
        "run_at": "document_end"
      },
      {
        "matches": ["https://*.nytimes.com/*opinion*"],
        "js": ["redirect.js"],
        "run_at": "document_end"
      }
  ]
}
